// Demo Library

Fun load_image2(path, gran)
{
	c <- load(path, gran);
	return c; 
}

Fun load_image(path)
{
	c <- load(path, 20);
	return c; 
}

Fun invert(c)
{
	z <- blank(c$h, c$w, c$g); 

	for i <- 0 | i < c$g | i <- i+1
	{
		d <- c[=i];
		d[=i] <- c$g-i-1;
		z <- z & d;
	}

	return z; 
}

Fun invert_loop(c)
{
	z <- c[,];

	for i <- 0 | i < z$h | i <- i+1
	{
		for k <- 0 | k < z$w | k <- k+1
		{
			z [i, k] <- (z$g - z[i, k] - 1);
		}
	}
	
	return z; 
}




