# FILENAME :  Makefile
# AUTHOR(S):  Joe Lee (jyl2157), Dmitriy Gromov (dg2720)
# PURPOSE  :  Makefile for EZ-ASCII project.

OBJS = ast.cmo scanner.cmo parser.cmo preprocess.cmo bytecode.cmo compiler.cmo hashtypes.cmo execute.cmo ezac.cmo

ezac : $(OBJS)
	ocamlc -o ezac  str.cma $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -c $<

%.cmi : %.mli
	ocamlc -c $<

.PHONY : clean
clean :
	rm -rf *.cmo *.cmi ezac parser.mli parser.ml scanner.ml

# generated by ocamldep *.ml *.mli
ezatypes.cmo :
ezatypes.cmx :
ast.cmo :
ast.cmx :
bytecode.cmo : hashtypes.cmo ast.cmo
bytecode.cmx : hashtypes.cmx ast.cmx
compiler.cmo : hashtypes.cmo ezatypes.cmo bytecode.cmo ast.cmo
compiler.cmx : hashtypes.cmx ezatypes.cmx bytecode.cmx ast.cmx
execute.cmo : hashtypes.cmo bytecode.cmo ast.cmo
execute.cmx : hashtypes.cmx bytecode.cmx ast.cmx
ezac.cmo : scanner.cmo preprocess.cmo parser.cmi execute.cmo compiler.cmo bytecode.cmo ast.cmo
ezac.cmx : scanner.cmx preprocess.cmx parser.cmx execute.cmx compiler.cmx bytecode.cmx ast.cmx
hashtypes.cmo :
hashtypes.cmx :
interpret.cmo : scanner.cmo parser.cmi ast.cmo
interpret.cmx : scanner.cmx parser.cmx ast.cmx
preprocess.cmo :
preprocess.cmx :
parser.cmo : ast.cmo parser.cmi
parser.cmx : ast.cmx parser.cmi
scanner.cmo : parser.cmi
scanner.cmx : parser.cmx
parser.cmi : ast.cmo
