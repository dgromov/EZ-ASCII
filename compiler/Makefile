OBJS = ast.cmo scanner.cmo parser.cmo ezac.cmo interpret.cmo 

ezac : $(OBJS)
	ocamlc -o ezac $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -c $<

%.cmi : %.mli
	ocamlc -c $<

.PHONY : clean
clean :
	rm -rf *.cmo *.cmi interpret ezac parser.mli parser.ml scanner.ml

# generated by ocamldep *.ml *.mli
ast.cmo :
ast.cmx :
bytecode.cmo : ast.cmo
bytecode.cmx : ast.cmx
ezac.cmo : interpret.cmo
ezac.cmx : interpret.cmx
interpret.cmo : scanner.cmo parser.cmi ast.cmo
interpret.cmx : scanner.cmx parser.cmx ast.cmx
parser.cmo : ast.cmo parser.cmi
parser.cmx : ast.cmx parser.cmi
parser.cmi : ast.cmo
scanner.cmo : parser.cmi
scanner.cmx : parser.cmx