OBJS = scanner.cmo parser.cmo interpreter.cmo 

interpreter : $(OBJS)
	ocamlc -o interpreter $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -c $<

%.cmi : %.mli
	ocamlc -c $<

.PHONY : clean
clean :
	rm -rf *.cmo *.cmi interpreter parser.mli parser.ml scanner.ml

# generated by ocamldep *.ml *.mli
interpeter.cmo : parser.cmi scanner.cmo 
interpeter.cmx : parser.cmx scanner.cmx
scanner.cmo : parser.cmi
scanner.cmx : parser.cmx
parser.cmo : parser.cmi
parser.cmx : parser.cmi
parser.cmi :
